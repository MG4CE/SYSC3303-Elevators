<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" packages="utils">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
    <PatternLayout pattern="%d{HH:mm:ss} %-5p %-42grp{[%t] %c} - %m%n" />
</Console>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>